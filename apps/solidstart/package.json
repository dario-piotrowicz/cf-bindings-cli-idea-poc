    "solid-start-node": "^0.3.10",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
		"wrangler": "https://prerelease-registry.devprod.cloudflare.dev/workers-sdk/runs/7035859586/npm-package-wrangler-4523"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@solidjs/meta": "^0.29.1",
    "@solidjs/router": "^0.8.3",
    "poc": "workspace:*",
